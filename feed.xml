---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
    <channel>
        <title>{{ site.data.settings.title | xml_escape }}</title>
        <description>{{ site.data.settings.description | xml_escape }}</description>
        <link>{{ site.url }}{{ site.baseurl }}/</link>
        <atom:link href="{{ "/feed.xml" | absolute_url }}" rel="self" type="application/rss+xml"/>
        <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
        <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
        <generator>Jekyll v{{ jekyll.version }}</generator>
        {% for publication in site.publications limit:10 %}
            <item>
                <title>{{ publication.title | xml_escape }}</title>
                <description>{{ publication.content | xml_escape }}</description>
                <pubDate>{{ publication.date | date_to_rfc822 }}</pubDate>
                <link>{{ publication.url | absolute_url }}</link>
                <guid isPermaLink="true">{{ publication.url | absolute_url }}</guid>
                {% for tag in publication.tags %}
                <category>{{ tag | xml_escape }}</category>
                {% endfor %}
                {% for cat in publication.categories %}
                <category>{{ cat | xml_escape }}</category>
                {% endfor %}
            </item>
        {% endfor %}
    </channel>
</rss>